{
  "expo": {
    "name": "Raksana",
    "slug": "raksana",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/raksana.png",
    "scheme": "raksana",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.j1rb.raksana"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/raksana-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.j1rb.raksana",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAqUtVevIw7S9XVbjHakVxgyBIsQNK-hwU"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/raksana.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video",
      [
        "expo-maps",
        {
          "googleMapsApiKey": "AIzaSyAqUtVevIw7S9XVbjHakVxgyBIsQNK-hwU"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a1851b12-ef6d-4c91-ab07-42b8cfe11b6d"
      }
    }
  }
}
